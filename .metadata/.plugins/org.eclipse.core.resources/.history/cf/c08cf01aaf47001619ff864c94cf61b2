package business;

import java.util.ArrayList;

public final class TransactionList {
	private static ArrayList<Transaction> transactionList;// = new ArrayList();
	
	public TransactionList()
	{
		transactionList = new ArrayList();
	}
	public static void addTransaction(String accountId, String transactionEntry, double amountTransaction, String descriptionTransaction)
	{
		transactionList.add(new Transaction(accountId, 
											transactionEntry, 
											amountTransaction,
											descriptionTransaction));
	}
}
